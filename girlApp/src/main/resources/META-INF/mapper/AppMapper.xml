<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.webapp.girlapp.mapper.AppMapper">
  <select id="queryList" resultType="App">
    select
    	  a.id as id,
          a.name as name
    from app a
    order by a.name
  </select>
</mapper>